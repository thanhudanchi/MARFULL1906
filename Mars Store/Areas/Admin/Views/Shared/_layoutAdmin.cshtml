    <!-- Bootstrap Core CSS -->
    <link href="/Areas/Admin/Razer/css/bootstrap.min.css" rel='stylesheet' type='text/css' />
    <!-- Custom CSS -->
    <link href="/Areas/Admin/Razer/css/style.css" rel='stylesheet' type='text/css' />
    <!-- Graph CSS -->
    <link href="/Areas/Admin/Razer/css/font-awesome.css" rel="stylesheet">
