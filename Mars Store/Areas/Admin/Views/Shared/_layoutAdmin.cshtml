            <div id="page-wrapper">
                @RenderBody()
            <!--body wrapper end-->
        </div>
        <!--footer section start-->
        <footer>
            @*<p>Coppy Right Of W3S. All Rights Reserved | Design by <a href="https://www.facebook.com/trandirector.iit" target="_blank">Di'rektə Trần.</a></p>*@
        </footer>
        <!--footer section end-->
        <!-- main content end-->
    </section>

    <script src="/Areas/Admin/Razer/js/jquery.nicescroll.js"></script>
    <script src="/Areas/Admin/Razer/js/scripts.js"></script>
    <!-- Bootstrap Core JavaScript -->
    <script src="/Areas/Admin/Razer/js/bootstrap.min.js"></script>
</body>
</html>