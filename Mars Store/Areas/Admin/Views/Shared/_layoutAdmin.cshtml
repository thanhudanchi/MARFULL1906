    <link href="/Areas/Admin/Razer/css/animate.css" rel="stylesheet" type="text/css" media="all">
    <script src="/Areas/Admin/Razer/js/wow.min.js"></script>
    <script>
        new WOW().init();
    </script>
    <!--//end-animate-->
    <!----webfonts--->
